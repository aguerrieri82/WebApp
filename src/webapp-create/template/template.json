{
  "name": "default",
  "source": "src",
  "transforms": [
    "package.json",
    "server.json",
    "public/index.html",
    "$(project-name).esproj",
    ".vscode/launch.json"
  ],
  "content": {
    "vs": [
      ".vscode",
      "$(project-name).esproj"
    ],
    "common": [
      "public",
      ".gitignore",
      "package.json",
      "rollup.config.js",
      "server.js"
    ],
    "ts": [
      "tsconfig.json"
    ]
  },
  "packages": {
    "ts": {
      "typescript": "^11.1.0",
      "tslib": "^2.5.0",
      "@rollup/plugin-typescript": "^5.0.4"
    }
  }
}