{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": false,
    "sourceMap": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "verbatimModuleSyntax": true
  },
  "compileOnSave": true
}
 
