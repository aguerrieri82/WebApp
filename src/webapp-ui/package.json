{
  "name": "@eusoft/webapp-ui",
  "version": "0.0.46",
  "packageManager": "pnpm@8.3.1",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "publish": "cd ../../dist/webapp-ui/ && pnpm publish --access public --no-git-checks",
    "clean": "rimraf ../../dist/webapp-ui/",
    "build:watch": "rollup -c -w  --environment NODE_ENV:dev",
    "build:dev": "rollup -c --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod",
    "lint:fix": "eslint ./src/**/*.ts* --fix",
    "install:pnpm": "pnpm install",
    "upgrade": "ncu --upgrade && pnpm install"
  },
  "keywords": [],
  "author": "Andrea Guerrieri",
  "devDependencies": {
    "@eusoft/webapp-compiler-rollup": "link:../../dist/webapp-compiler-rollup",
    "build-utils": "link:../../scripts/build-utils",
    "rollup": "^3.23.0",
    "eslint": "^8.41.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "@typescript-eslint/parser": "5.59.8",
    "eslint-config-standard-with-typescript": "34.0.1",
    "npm-check-updates": "16.10.12",
    "rimraf": "^5.0.1"
  },
  "dependencies": {
    "@eusoft/webapp-core": "link:../webapp-core/src",
    "@eusoft/webapp-jsx": "link:../webapp-jsx/src"
  }
}