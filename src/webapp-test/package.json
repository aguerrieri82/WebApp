{
  "name": "webapp-test",
  "version": "0.0.1",
  "packageManager": "pnpm@8.4.0",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node server",
    "dev": "rollup -c -w --environment NODE_ENV:dev",
    "dist:dev": "cd ../../scripts/build-utils/ && node dist",
    "dist:prod": "cd ../../scripts/build-utils/ && node dist -prod",
    "dist:pub": "cd ../../scripts/build-utils/ && node dist -n -p",
    "build:dev": "rollup -c --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod",
    "install:pnpm": "pnpm install",
    "upgrade": "pnpm up -latest"
  },
  "keywords": [],
  "author": "Andrea Guerrieri",
  "dependencies": {
    "@eusoft/webapp-core": "link:../webapp-core/src",
    "@eusoft/webapp-jsx": "link:../webapp-jsx/src",
    "@eusoft/webapp-ui": "link:../webapp-ui/src",
    "@eusoft/webapp-framework": "link:../webapp-framework/src"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@eusoft/webapp-compiler-rollup": "../../dist/webapp-compiler-rollup",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/plugin-terser": "0.4.1",
    "rollup-plugin-livereload": "2.0.5",
    "express": "4.18.2",
    "rollup": "^3.21.5",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-scss": "4.0.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "sass": "1.62.1",
    "tslib": "^2.5.0",
    "typescript": "5.0.4"
  }
