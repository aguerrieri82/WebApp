{
  "name": "@eusoft/webapp-compiler",
  "version": "0.0.54",
  "packageManager": "pnpm@8.4.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "publish": "cd ../../dist/webapp-compiler/ && pnpm publish --access public --no-git-checks",
    "clean": "rimraf ../../dist/webapp-compiler/",
    "build:watch": "rollup -c -w  --environment NODE_ENV:dev",
    "build:dev": "rollup -c --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod",
    "lint:fix": "eslint ./src/**/*.ts* --fix",
    "install:pnpm": "pnpm install",
    "upgrade": "pnpm up -latest"
  },
  "keywords": [],
  "author": "Andrea Guerrieri",
  "dependencies": {
    "@babel/core": "7.23.2",
    "@babel/parser": "7.23.0",
    "@babel/traverse": "7.23.2",
    "@babel/types": "7.23.0",
    "jsdom": "22.1.0"
  },
  "devDependencies": {
    "@types/babel__core": "7.20.3",
    "@types/babel__traverse": "7.20.3",
    "@types/jsdom": "21.1.4",
    "@types/node": "20.8.7",
    "@typescript-eslint/parser": "6.8.0",
    "build-utils": "link:../../scripts/build-utils",
    "eslint": "^8.52.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4"
  }
}