{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": false,
    "allowJs": true,
    "sourceMap": true,
    "target": "ESNext",
    "module": "ESNext",
    "declaration": true,
    "moduleResolution": "Node",
    "noEmit": true,
    "experimentalDecorators": true,
    "noImplicitOverride": true,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true
  },
  "include": ["src"]
}
 
