{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": false,
    "sourceMap": true,
    "target": "ESNext",
    "module": "ESNext",
    "declaration": true,
    "declarationDir": "types",
    "moduleResolution": "Node",
    "noEmit": true,
    "incremental": true,
    "noImplicitOverride": true,
    "composite": true
  },
  "include": ["src"]
}
 
