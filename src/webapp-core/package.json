{
  "name": "@eusoft/webapp-core",
  "version": "0.0.14",
  "packageManager": "pnpm@8.4.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "publish": "cd ../../dist/webapp-core/ && pnpm publish --access public --no-git-checks",
    "clean": "rimraf ../../dist/webapp-core/",
    "build:watch": "rollup -c -w --environment NODE_ENV:dev",
    "build:dev": "rollup -c --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod",
    "lint:fix": "eslint ./src/**/*.ts* --fix",
    "install:pnpm": "pnpm install",
    "upgrade": "pnpm up -latest"
  },
  "keywords": [],
  "author": "Andrea Guerrieri",
  "devDependencies": {
    "@typescript-eslint/parser": "5.59.2",
    "build-utils": "link:../../scripts/build-utils",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.5"
  }
}