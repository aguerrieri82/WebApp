{
  "name": "@eusoft/webapp-framework",
  "version": "0.0.18",
  "packageManager": "pnpm@8.3.1",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "publish": "cd ../../dist/webapp-framework/ && pnpm publish --access public --no-git-checks",
    "clean": "rimraf ../../dist/webapp-framework/",
    "build:watch": "rollup -c -w  --environment NODE_ENV:dev",
    "build:dev": "rollup -c --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod",
    "lint:fix": "eslint ./src/**/*.ts* --fix",
    "install:pnpm": "pnpm install",
    "upgrade": "pnpm up -latest"
  },
  "keywords": [],
  "author": "Andrea Guerrieri",
  "devDependencies": {
    "@eusoft/webapp-compiler-rollup": "link:../../dist/webapp-compiler-rollup",
    "@typescript-eslint/parser": "6.8.0",
    "build-utils": "link:../../scripts/build-utils",
    "eslint": "^8.52.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4"
  },
  "dependencies": {
    "@eusoft/webapp-core": "link:../webapp-core/src",
    "@eusoft/webapp-jsx": "link:../webapp-jsx/src",
    "@eusoft/webapp-ui": "link:../webapp-ui/src"
  }
}