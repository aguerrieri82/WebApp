{
  "name": "@eusoft/webapp-jsx",
  "version": "0.0.53",
  "packageManager": "pnpm@8.4.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "scripts": {
    "publish": "cd ../../dist/webapp-jsx/ && pnpm publish --access public --no-git-checks",
    "clean": "rimraf ../../dist/webapp-jsx/",
    "build:watch": "rollup -c -w  --environment NODE_ENV:dev",
    "build:dev": "rollup -c --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod",
    "lint:fix": "eslint ./src/**/*.ts* --fix",
    "install:pnpm": "pnpm install",
    "upgrade": "pnpm up -latest"
  },
  "keywords": [],
  "author": "Andrea Guerrieri",
  "devDependencies": {
    "@typescript-eslint/parser": "6.4.1",
    "build-utils": "link:../../scripts/build-utils",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "38.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1"
  },
  "dependencies": {
    "@eusoft/webapp-core": "link:../webapp-core/src"
  }
}