{
  "name": "@eusoft/create-webapp",
  "version": "0.0.14",
  "packageManager": "pnpm@8.4.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build:dev": "node build",
    "build:prod": "pnpm run clean&&node build",
    "clean": "rimraf ../../dist/create-webapp/",
    "publish": "cd ../../dist/create-webapp/ && pnpm publish --access public --no-git-checks",
    "lint:fix": "eslint ./src/**/*.ts* --fix",
    "install:pnpm": "pnpm install",
    "upgrade": "pnpm up -latest"
  },
  "keywords": [],
  "author": "Andrea Guerrieri",
  "dependencies": {
    "open": "9.1.0"
  },
  "devDependencies": {
    "@types/node": "20.1.1",
    "@types/fs-extra": "11.0.1",
    "typescript": "5.0.4",
    "fs-extra": "11.1.1",
    "rimraf": "^5.0.0",
    "build-utils": "link:../../scripts/build-utils"
  }
}