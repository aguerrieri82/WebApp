{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [ "<node_internals>/**" ],
      "sourceMaps": true,
      "program": "dist/index.js",
      "args": [ "--build", "--lib:../../src/webapp-compiler-rollup", "--dist", "--env:prod", "--publish", "--new" ],
      "cwd": "${workspaceFolder}",
      "console": "externalTerminal"
    }
  ]
}